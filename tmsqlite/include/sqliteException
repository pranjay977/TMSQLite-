#ifndef __$__sqliteException
#define __$__sqliteException 1234
#include<iostream>
using namespace std;
class SQLiteException:public exception
{
private:
string message;
public:
SQLiteException();
SQLiteException(const  SQLiteException &other);
SQLiteException(string message);
SQLiteException(string message1,string message2);
virtual ~SQLiteException();
SQLiteException & operator=(const SQLiteException &other);
const char *what() const throw();
};
#endif